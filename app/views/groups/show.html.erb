<div class="container content profile">
    <div class="row">
        <div class="col-md-3 md-margin-bottom-40">
            <%= render 'shared/menus/group' %>
        </div>
        <div class="col-md-9">
            <div class="profile-body">

                <h1>
                    <%for group in @group.mothers%>
                        <%=link_to(group.name,group.path)%> ::
                    <%end%>
                    <%if @group.acronym != ""%>
                        <%=@group.acronym%> -
                    <%end%>
                    <%=@group.name%>
                </h1>

                <p><%=@group.description%></p>

                <div class="panel panel-profile">
                    <div class="panel-heading overflow-h">
                        <h2 class="panel-title heading-sm pull-left"><i class="fa fa-tasks"></i><%=@title%></h2>

                        <a href="" class="btn-u btn-brd btn-brd-hover btn-u-dark btn-u-xs pull-right">View All</a>
                    </div>

                    <div class="panel-body">
                        <div class="row">

							<%@objects.each do |node|%>
                                <div class="col-sm-6">
                                    <div class="profile-blog blog-border">
                                        <img src="<%=node.path_thumbnail%>" alt="">
                                        <div class="name-location">
                                            <strong><%=link_to(node.name,node.path)%></strong>
                                            <span></span>
                                        </div>
                                    </div>
                                </div>
                            <%end%>

                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
